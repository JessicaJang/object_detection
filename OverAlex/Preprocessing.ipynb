    "def preprocessing_BG(dataset_dir, subset, patch_size=64):\n",
    "    \n",
    "    assert subset in [\"train\", \"val\"]\n",
    "    dataset_dir = os.path.join(dataset_dir, subset)\n",
    "    \n",
    "    annotations = json.load(open(os.path.join(dataset_dir,\"label.json\")))\n",
    "    for a in annotations: # each images\n",
    "        label = a['Label']['objects']\n",
    "        print(a['External ID'])\n",
    "        image_path = os.path.join(dataset_dir,a['External ID'])\n",
    "        image = skimage.io.imread(image_path)\n",
    "        # print(label)\n",
    "        oh, ow = image.shape[:2]\n",
    "        print(\"Original height, width\", oh, ow)\n",
    "        \n",
    "        scale = max(1, 64 / min(oh, ow))\n",
    "        max_h, max_w = oh, ow\n",
    "\n",
    "        # Height - image\n",
    "        if oh % 64 > 0:\n",
    "            max_h = oh - (oh % 64) + 64\n",
    "            top_pad = (max_h - oh) // 2\n",
    "            bottom_pad = max_h - oh - top_pad\n",
    "        else:\n",
    "            top_pad = bottom_pad = 0\n",
    "    \n",
    "        # Width - image\n",
    "        if ow % 64 > 0:\n",
    "            max_w = ow - (ow % 64) + 64\n",
    "            left_pad = (max_w - ow) // 2\n",
    "            right_pad = max_w - ow - left_pad\n",
    "        else:\n",
    "            left_pad = right_pad = 0\n",
    "        \n",
    "        padding = [(top_pad, bottom_pad), (left_pad, right_pad), (0, 0)]\n",
    "        image = np.pad(image, padding, mode='constant', constant_values=0)\n",
    "        \n",
    "        m, n = max_h // 64, max_w // 64\n",
    "        print(m ,n)\n",
    "        patch_size = 64    \n",
    "        for j in range(n):\n",
    "            for i in range(m):\n",
    "                new_h1, new_h2 = (patch_size*i), (patch_size*(i+1))\n",
    "                new_w1, new_w2 = (patch_size*j), (patch_size*(j+1))\n",
    "                new_image = image[new_h1 : new_h2, new_w1 : new_w2] # 64 patch\n",
    "                crack_num = 0\n",
    "                for r in label: # each crack\n",
    "                    if r['title'] == 'crack' and r['line']:\n",
    "                        for k in r['line']:\n",
    "                            if (k['x']>=new_w1 and k['x']<=new_w2) and (k['y']>=new_h1 and k['y']<=new_h2): crack_num += 1\n",
    "                if crack_num == 0:\n",
    "                    new_filename = a['External ID'].split('.')[0] + \"_\" + str(crack_num) + \"_\" + str(i) + str(j) + \".jpg\"\n",
    "                    dest_dir = dataset_dir+\"/BG_temp/\"+new_filename\n",
    "                    cv2.imwrite(dest_dir, new_image)\n",
    "                                \n",
    "                        "
